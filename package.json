{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate with the most basic setup that i like to start with.",
  "main": "index.js",
  "scripts": {
    "check": "yarn outdated -depth 0 --long",
    "clean-install": "rm -rf node_modules && yarn",
    "build": "webpack -p --config webpack/production.config.js",
    "start": "webpack-dev-server --inline --hot --config webpack/dev.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "precommit": "lint-staged",
    "lint": "./node_modules/.bin/eslint ./app/**/*",
    "pretify": "node_modules/.bin/prettier --write -l 'app/**/*.{js,jsx}'"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|pcss)$": "identity-obj-proxy"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "node_modules/.bin/prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:khizar/react-boilerplate.git"
  },
  "author": "khizar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khizar/react-boilerplate/issues"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@hot-loader/react-dom": "^16.13.0",
    "classnames": "^2.2.6",
    "postcss-bem-fix": "^2.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "unfetch": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime-corejs2": "^7.0.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "autoprefixer": "^9.7.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^23.4.2",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-config-standard-with-typescript": "^15.0.1",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.4.0",
    "lint-staged": "^10.1.7",
    "postcss": "^7.0.27",
    "postcss-calc": "^7.0.2",
    "postcss-color-function": "^4.1.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^9.1.1",
    "postcss-custom-selectors": "^5.1.2",
    "postcss-em-media-query": "^2.0.0",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-media-minmax": "^4.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-nesting": "^7.0.1",
    "postcss-scss": "^2.0.0",
    "postcss-selector-matches": "^4.0.0",
    "postcss-url": "^8.0.0",
    "precss": "^4.0.0",
    "prettier": "^2.0.5",
    "react-css-modules": "^4.7.11",
    "react-hot-loader": "^4.12.20",
    "react-test-renderer": "^16.13.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.4",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "homepage": "https://github.com/khizar/react-boilerplate#readme"
}
