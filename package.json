{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate with the most basic setup that i like to start with.",
  "main": "index.js",
  "scripts": {
    "clean-install": "rm -rf node_modules && npm install",
    "build": "webpack -p --config webpack/production.config.js",
    "start": "webpack-dev-server --inline --hot --config webpack/dev.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "precommit": "lint-staged",
    "eslint": "./node_modules/.bin/eslint /app/",
    "pretify": "node_modules/.bin/prettier --print-width 100 --single-quote --tab-width 4 --write -l 'app/**/*.{js,jsx}'"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|pcss)$": "identity-obj-proxy"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "node_modules/.bin/prettier --print-width 100 --single-quote --tab-width 4 --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khizar/twitter-reading-list.git"
  },
  "author": "khizar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khizar/twitter-reading-list/issues"
  },
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.28.5",
    "enzyme": "^2.9.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-prettier": "^2.2.0",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.2.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.3",
    "postcss": "^6.0.9",
    "postcss-bem": "^0.4.1",
    "postcss-calc": "^6.0.0",
    "postcss-color-function": "^4.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.1.0",
    "postcss-custom-selectors": "^4.0.1",
    "postcss-em-media-query": "^2.0.0",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-media-minmax": "^3.0.0",
    "postcss-nested": "^2.1.2",
    "postcss-nesting": "^4.0.1",
    "postcss-scss": "^1.0.2",
    "postcss-selector-matches": "^3.0.1",
    "postcss-url": "^7.1.2",
    "precss": "^2.0.0",
    "prettier": "^1.5.3",
    "react-css-modules": "^4.7.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^15.6.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.7.1",
    "whatwg-fetch": "^2.0.3"
  },
  "homepage": "https://github.com/khizar/twitter-reading-list#readme"
}
